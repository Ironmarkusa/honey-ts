{
  "name": "honey-ts",
  "version": "0.1.0",
  "description": "HoneySQL for TypeScript - SQL as data structures",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./pg-ops": {
      "types": "./dist/pg-ops.d.ts",
      "import": "./dist/pg-ops.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "test": "node --test --import tsx src/**/*.test.ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "sql",
    "honeysql",
    "query-builder",
    "postgres",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "pgsql-ast-parser": "^12.0.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^25.2.0",
    "fast-check": "^4.5.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
